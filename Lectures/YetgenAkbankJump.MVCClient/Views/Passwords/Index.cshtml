@model PasswordsIndexViewModel
<h1>Şifreniz : @Model.Password</h1>

<form asp-action="Index" asp-route-viewModel="@Model.PasswordLength">

    <label>Şifre uzunluğunu giriniz : </label>
    <input class="form-control" asp-for="PasswordLength" />

    <button class="btn btn-success mt-3" type="submit"> Kaydet </button>

</form>

